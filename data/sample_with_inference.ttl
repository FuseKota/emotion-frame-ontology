@prefix ex: <http://example.org/data#> .
@prefix pl: <http://example.org/efo/plutchik#> .
@prefix emo: <http://www.ontologydesignpatterns.org/ont/emotions/EmoCore.owl#> .
@prefix fschema: <https://w3id.org/framester/schema/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Sample data for Plutchik Dyad inference testing
# Each FrameOccurrence has 2 EmotionEvidence nodes with scores
# INFERENCE RESULTS INCLUDED (TH=0.4)

#################################################################
# ex:s1 - Love (Joy + Trust), score=min(0.80, 0.70)=0.70
#################################################################
ex:s1 rdf:type fschema:FrameOccurrence ;
    rdfs:label "Situation 1: Love scenario" ;
    pl:hasEvidence ex:s1_ev_joy , ex:s1_ev_trust , ex:s1_ev_love_inferred ;
    pl:satisfiesEmotion pl:Love .

ex:s1_ev_joy rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Joy ;
    pl:score "0.80"^^xsd:decimal .

ex:s1_ev_trust rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Trust ;
    pl:score "0.70"^^xsd:decimal .

ex:s1_ev_love_inferred rdf:type pl:EmotionEvidence ;
    rdfs:label "Inferred: Love" ;
    pl:emotion pl:Love ;
    pl:score "0.70"^^xsd:decimal ;
    pl:derivedFrom ex:s1_ev_joy , ex:s1_ev_trust ;
    pl:inferenceMethod "min-threshold-0.4" .

#################################################################
# ex:s2 - Contempt (Disgust + Anger), score=min(0.60, 0.60)=0.60
#################################################################
ex:s2 rdf:type fschema:FrameOccurrence ;
    rdfs:label "Situation 2: Contempt scenario" ;
    pl:hasEvidence ex:s2_ev_disgust , ex:s2_ev_anger , ex:s2_ev_contempt_inferred ;
    pl:satisfiesEmotion pl:Contempt .

ex:s2_ev_disgust rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Disgust ;
    pl:score "0.60"^^xsd:decimal .

ex:s2_ev_anger rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Anger ;
    pl:score "0.60"^^xsd:decimal .

ex:s2_ev_contempt_inferred rdf:type pl:EmotionEvidence ;
    rdfs:label "Inferred: Contempt" ;
    pl:emotion pl:Contempt ;
    pl:score "0.60"^^xsd:decimal ;
    pl:derivedFrom ex:s2_ev_disgust , ex:s2_ev_anger ;
    pl:inferenceMethod "min-threshold-0.4" .

#################################################################
# ex:s3 - Aggressiveness (Anger + Anticipation), score=min(0.90, 0.50)=0.50
#################################################################
ex:s3 rdf:type fschema:FrameOccurrence ;
    rdfs:label "Situation 3: Aggressiveness scenario" ;
    pl:hasEvidence ex:s3_ev_anger , ex:s3_ev_anticipation , ex:s3_ev_aggressiveness_inferred ;
    pl:satisfiesEmotion pl:Aggressiveness .

ex:s3_ev_anger rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Anger ;
    pl:score "0.90"^^xsd:decimal .

ex:s3_ev_anticipation rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Anticipation ;
    pl:score "0.50"^^xsd:decimal .

ex:s3_ev_aggressiveness_inferred rdf:type pl:EmotionEvidence ;
    rdfs:label "Inferred: Aggressiveness" ;
    pl:emotion pl:Aggressiveness ;
    pl:score "0.50"^^xsd:decimal ;
    pl:derivedFrom ex:s3_ev_anger , ex:s3_ev_anticipation ;
    pl:inferenceMethod "min-threshold-0.4" .

#################################################################
# ex:s4 - Disapproval (Surprise + Sadness), score=min(0.60, 0.45)=0.45
#################################################################
ex:s4 rdf:type fschema:FrameOccurrence ;
    rdfs:label "Situation 4: Disapproval scenario" ;
    pl:hasEvidence ex:s4_ev_surprise , ex:s4_ev_sadness , ex:s4_ev_disapproval_inferred ;
    pl:satisfiesEmotion pl:Disapproval .

ex:s4_ev_surprise rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Surprise ;
    pl:score "0.60"^^xsd:decimal .

ex:s4_ev_sadness rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Sadness ;
    pl:score "0.45"^^xsd:decimal .

ex:s4_ev_disapproval_inferred rdf:type pl:EmotionEvidence ;
    rdfs:label "Inferred: Disapproval" ;
    pl:emotion pl:Disapproval ;
    pl:score "0.45"^^xsd:decimal ;
    pl:derivedFrom ex:s4_ev_surprise , ex:s4_ev_sadness ;
    pl:inferenceMethod "min-threshold-0.4" .

#################################################################
# ex:s5 - Hope (Anticipation + Trust), score=min(0.42, 0.41)=0.41
#################################################################
ex:s5 rdf:type fschema:FrameOccurrence ;
    rdfs:label "Situation 5: Hope scenario (boundary case)" ;
    pl:hasEvidence ex:s5_ev_anticipation , ex:s5_ev_trust , ex:s5_ev_hope_inferred ;
    pl:satisfiesEmotion pl:Hope .

ex:s5_ev_anticipation rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Anticipation ;
    pl:score "0.42"^^xsd:decimal .

ex:s5_ev_trust rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Trust ;
    pl:score "0.41"^^xsd:decimal .

ex:s5_ev_hope_inferred rdf:type pl:EmotionEvidence ;
    rdfs:label "Inferred: Hope" ;
    pl:emotion pl:Hope ;
    pl:score "0.41"^^xsd:decimal ;
    pl:derivedFrom ex:s5_ev_anticipation , ex:s5_ev_trust ;
    pl:inferenceMethod "min-threshold-0.4" .

#################################################################
# ex:s6 - NO dyad inferred (Fear=0.39 < TH=0.4)
# Awe = Fear + Surprise, but Fear score is below threshold
#################################################################
ex:s6 rdf:type fschema:FrameOccurrence ;
    rdfs:label "Situation 6: Threshold test (no dyad expected)" ;
    rdfs:comment "Fear=0.39 is below threshold 0.4, so Awe is NOT inferred" ;
    pl:hasEvidence ex:s6_ev_fear , ex:s6_ev_surprise .

ex:s6_ev_fear rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Fear ;
    pl:score "0.39"^^xsd:decimal .

ex:s6_ev_surprise rdf:type pl:EmotionEvidence ;
    pl:emotion pl:Surprise ;
    pl:score "0.80"^^xsd:decimal .
